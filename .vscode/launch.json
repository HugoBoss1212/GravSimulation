{
    "name": "CMake Debug",
    "type": "cppdbg",
    "request": "launch",
    "program": "${command:cmake.launchTargetPath}",
    "args": [],
    "stopAtEntry": false,
    "cwd": "${workspaceFolder}",
    "environment": [],
    "externalConsole": false,
    "MIMode": "gdb",
    "setupCommands": [
      {
        "description": "Enable pretty-printing for gdb",
        "text": "-enable-pretty-printing",
        "ignoreFailures": true
      }
    ],
    "miDebuggerPath": "/usr/bin/gdb",
    "miDebuggerArgs": "",
    "sourceFileMap": {
      "/mnt/c": "C:\\"
    },
    "preLaunchTask": "",
    "logging": {
      "trace": false,
      "traceResponse": false,
      "engineLogging": false
    }
  }
  